#: ../src/mps-feed-pane.c:92
msgid ""
"Unable to update status. The service may be unavailable or your password "
"could be wrong."
msgstr ""
"No s'ha pogut actualitzar l'estat. Pot ser que el servei no estigui "
"disponible o que la contrasenya no sigui correcta."

#: ../src/mps-feed-pane.c:96
msgid "Unable to update status: You're not online."
msgstr "No s'ha pogut actualitzar l'estat: no esteu en línia."

#: ../src/mps-feed-pane.c:445
msgid "We were unable to guess your location"
msgstr "No s'ha pogut estimar la vostra ubicació"

#: ../src/mps-feed-pane.c:453
#, c-format
msgid "We think your location is: %s (%f, %f)"
msgstr "Creiem que la vostra ubicació és: %s (%f, %f)"

#: ../src/mps-feed-pane.c:459
#, c-format
msgid "Your location is currently shared as: %s (%f, %f)"
msgstr "S'està compartint la vostra ubicació actualment com a: %s (%f, %f)"

#: ../src/mps-feed-pane.c:469
#, c-format
msgid "We think your location is: %f %f"
msgstr "Creiem que la vostra ubicació és: %f %f"

#: ../src/mps-feed-pane.c:473
#, c-format
msgid "Your location is currently shared as: %f %f"
msgstr "S'està compartint la vostra ubicació actualment com a: %f %f"

#: ../src/mps-feed-pane.c:483
msgid "Your location isn't currently shared"
msgstr "No s'està compartint la vostra ubicació actualment"

